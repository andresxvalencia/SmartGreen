<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cardview_light_background"
    tools:context=".mostrarDatos">

    <Button
        android:id="@+id/btnNuevoValor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/button_rounded"
        android:backgroundTint="#0ee889"
        android:text="NUEVO"
        tools:layout_editor_absoluteX="290dp"
        tools:layout_editor_absoluteY="553dp" />

    <Button
        android:id="@+id/btnVincular"
        android:layout_width="201dp"
        android:layout_height="30dp"
        android:background="@drawable/button_rounded"
        android:backgroundTint="#0ee889"
        android:text="VINCULAR"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="621dp" />

    <Button
        android:id="@+id/btnCerrarSesion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/button_rounded"
        android:backgroundTint="#0ee889"
        android:text="SALIR"
        tools:layout_editor_absoluteX="288dp"
        tools:layout_editor_absoluteY="623dp" />

    <ImageButton
        android:id="@+id/btnSubir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/button_rounded2"
        android:minWidth="48dp"
        android:minHeight="48dp"
        app:srcCompat="@android:drawable/ic_menu_upload"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="548dp" />

    <ImageButton
        android:id="@+id/btnCompartir"
        android:layout_width="26dp"
        android:layout_height="47dp"
        android:background="@drawable/button_rounded2"
        app:srcCompat="@android:drawable/ic_menu_share"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="553dp" />

    <ImageButton
        android:id="@+id/btnEliminar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/button_rounded2"
        app:srcCompat="@android:drawable/ic_menu_delete"
        tools:layout_editor_absoluteX="227dp"
        tools:layout_editor_absoluteY="557dp" />

    <TextView
        android:id="@+id/Datos"
        android:layout_width="289dp"
        android:layout_height="50dp"
        android:text="Prueba"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.073" />

    <TextView
        android:id="@+id/deviceName"
        android:layout_width="116dp"
        android:layout_height="36dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="135dp" />

    <TextView
        android:id="@+id/deviceAddress"
        android:layout_width="92dp"
        android:layout_height="36dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="257dp"
        tools:layout_editor_absoluteY="135dp" />
</androidx.constraintlayout.widget.ConstraintLayout>